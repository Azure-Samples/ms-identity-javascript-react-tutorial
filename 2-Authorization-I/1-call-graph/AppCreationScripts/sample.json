{
    "Sample": {
        "Title": "React single-page application using MSAL React to call the Microsoft Graph API",
        "Level": 100,
        "Client": "React SPA",
        "RepositoryUrl": "ms-identity-javascript-react-tutorial",
        "Endpoint": "AAD v2.0"
    },
    "ReadmeScenario": {
        "FreeText": "",
        "IncludeFilePath": "",
        "UseNewSetup": "1",
        "CertificateOption": "0",
        "CreateProjectIncludeFilePath": "",
        "AppRegistrationIncludeFilePath": "",
        "RunSampleIncludeFilePath": ""
    },
    "AADApps": [
        {
            "Id": "spa",
            "Name": "ms-identity-react-c2s1",
            "Kind": "SinglePageApplication",
            "HomePage": "http://localhost:3000/",
            "ReplyUrls": "http://localhost:3000/, http://localhost:3000/redirect.html",
            "Audience": "AzureADMyOrg",
            "RequiredResourcesAccess": [
                {
                    "Resource": "Microsoft Graph",
                    "DelegatedPermissions": ["User.Read", "Contacts.Read"]
                }
            ],
            "OptionalClaims": {
                "IdTokenClaims": ["sid", "login_hint", "email", "upn", "acct"]
            }
        }
    ],
    "CodeConfiguration": [
        {
            "App": "spa",
            "SettingKind": "Replace",
            "SettingFile": "\\..\\SPA\\src\\authConfig.js",
            "Mappings": [
                {
                    "key": "Enter_the_Application_Id_Here",
                    "value": ".AppId"
                },
                {
                    "key": "Enter_the_Tenant_Info_Here",
                    "value": "$tenantId"
                }
            ]
        }
    ]
}
