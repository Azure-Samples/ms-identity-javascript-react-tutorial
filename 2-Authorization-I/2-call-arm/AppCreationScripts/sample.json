{
    "Sample": {
        "Title": "React single-page application using MSAL React to sign-in users and call Azure REST API and Azure Storage",
        "Level": 100,
        "Client": "React SPA",
        "Service": "",
        "RepositoryUrl": "ms-identity-javascript-react-tutorial",
        "Endpoint": "AAD v2.0",
        "Languages": ["javascript", "react"],
        "Products": ["azure-active-directory", "msal-react"],
        "description": "This sample demonstrates a React single-page application (SPA) that signs-in users with Azure AD and calls the [Azure Resource Manager API](https://docs.microsoft.com/en-us/rest/api/resources) and [Azure Storage API](https://docs.microsoft.com/en-us/rest/api/storageservices/) using the [Microsoft Authentication Library for React](https://github.com/AzureAD/microsoft-authentication-library-for-js/tree/dev/lib/msal-react) (MSAL React)."
    },
    "AADApps": [
        {
            "Id": "spa",
            "Name": "ms-identity-react-c2s2",
            "Kind": "SinglePageApplication",
            "HomePage": "http://localhost:3000/",
            "ReplyUrls": "http://localhost:3000/, http://localhost:3000/redirect.html",
            "Audience": "AzureADMyOrg",
            "Sample": {
                "SampleSubPath": "2-Authorization-I\\2-call-arm",
                "ProjectDirectory": "\\SPA"
            },
            "RequiredResourcesAccess": [
                {
                    "Resource": "Windows Azure Service Management API",
                    "DelegatedPermissions": ["user_impersonation"]
                },
                {
                    "Resource": "Azure Storage",
                    "DelegatedPermissions": ["user_impersonation"]
                }
            ]
        }
    ],
    "CodeConfiguration": [
        {
            "App": "spa",
            "SettingKind": "Replace",
            "SettingFile": "\\..\\SPA\\src\\authConfig.js",
            "Mappings": [
                {
                    "key": "Enter_the_Application_Id_Here",
                    "value": ".AppId"
                },
                {
                    "key": "Enter_the_Tenant_Info_Here",
                    "value": "$tenantId"
                }
            ]
        }
    ]
}



