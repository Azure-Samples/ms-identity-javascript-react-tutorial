{
    "Sample": {
        "Title": "React single-page application using MSAL React to authenticate users against Azure Active Directory",
        "Level": 100,
        "Client": "React SPA",
        "RepositoryUrl": "ms-identity-javascript-react-tutorial",
        "Endpoint": "AAD v2.0",
        "Languages": ["javascript"],
        "Description": "React single-page application using MSAL React to authenticate users against Azure Active Directory",
        "products": ["azure-active-directory", "msal-js", "msal-react"],
        "Platform": "javascript"
    },
    "AADApps": [
        {
            "Id": "client",
            "Name": "msal-react-spa",
            "Kind": "SinglePageApplication",
            "HomePage": "http://localhost:3000/",
            "ReplyUrls": "http://localhost:3000/, http://localhost:3000/redirect",
            "Audience": "AzureADMyOrg", 
            "SDK": "MsalJs",
            "SampleSubPath": "1-Authentication\\1-sign-in\\SPA"
        }
    ],
    "CodeConfiguration": [
        {
            "App": "client",
            "SettingKind": "Replace",
            "SettingFile": "\\..\\SPA\\src\\authConfig.js",
            "Mappings": [
                {
                    "key": "Enter_the_Application_Id_Here",
                    "value": ".AppId"
                },
                {
                    "key": "Enter_the_Tenant_Id_Here",
                    "value": "$tenantId"
                }
            ]
        }
    ]
}